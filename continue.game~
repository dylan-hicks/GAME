class Dude {
	num age
	text name
	bool isCool
	list(num) p

	function build(num a) {
		age = a
		height = "house"
		name = "will"
		p = {1, 2, 3, 4}
		p.add(5)
		p.addAt(1, 10)

		loop (start num j = 0, while j <= 4, set j = j + 1) {
			temp = p.get(j)
			print(temp)
		}	
	}
      
	function addTen() {
		age = age + 10
		removed = p.rem(1)
		print(removed)
		p.remAt(1)
	}
}

function main() {
	Dude bob = new Dude
	bob.build(12)
	bob.addTen()

}
