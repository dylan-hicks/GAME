class Dude {
	num age
	text name
	bool isCool
	list(num) p

	function build(num a) {
		age = a
		height = "house"
		name = "will"
		p = {1, 2, 3, 4}
		p.add(5)	
	}
      
	function addTen() {
		age = age + 10
		p.remove(1)
	}
}

function main() {
	Dude bob = new Dude
	bob.build(12)
	bob.addTen()

	Dude andy = new Dude
	andy.build(15)
	andy.addTen()
	print(andy.age)
	andy.addTen()
	print(bob.age)
	print(andy.age)
	print(bob.p)
	print(andy.p)
}
