class Dude{
    num age
    text name
    bool isCool
    
    function build(num a, text n, bool i){
        age = a
        name = n
        isCool = i
    }
    
    function addTen(){
        age = age + 10
    }

    bool function compareTo(Dude two){
    bool result
    if(age==two.age and name==two.name and isCool==two.isCool){
        result = true
    } else {
        result = false
    }
    return result
    }
}    

function main(){
    Dude bob = new Dude
    bob.build(12, "Bob", false)
    #bob.addTen()
    list(num) lll = {1,2,3,4}
    print(lll)
    
    Dude john = new Dude
    john.build(14, "John", true)
    print("Bob's age: " + bob.age)
    print("Bob's name: " + bob.name)
    print("Is Bob cool? " + bob.isCool)

    if (john.age > bob.age){
        print(john.name + " is older than " + bob.name)
    } else{
        print(bob.name + " is older than " + john.name)
    }
    print(bob.compareTo(john))        
} 
